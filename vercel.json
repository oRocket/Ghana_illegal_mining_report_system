{
  "version": 2,
  "builds": [
    {
      "src": "illegal_mining_report/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "illegal_mining_report/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
